@model IEnumerable<Entities.clsMision>

@{
    ViewData["Title"] = "ListaMisiones";
}

<h1>ListaMisiones</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                MISIÓN
            </th>
            <th>
                CRÉDITOS
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreMision)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Creditos)
            </td>
            <td>
                @Html.ActionLink("Detalles", "Details", new { id = item.Id }) |
            </td>
        </tr>
}
    </tbody>
</table>
